{
    "aliases": {},
    "order": 0,
    "settings": {
        "analysis": {
            "analyzer": {
                "custom_whitespace": {
                    "filter": ["lowercase"],
                    "tokenizer": "whitespace"
                }
            }
        },
        "index.analysis.analyzer.default.stopwords": "_none_",
        "index.analysis.analyzer.default.type": "standard",
        "index.refresh_interval": "5s"
    },
    "template": "goldstone-*",
    "mappings": {
        "openstack_service": {
            "_timestamp": {
                "enabled": true
            },
            "_source": {
                "enabled": true
            },
            "_id": {
                "path": "id"
            },
            "properties": {
                "id": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "name": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "description": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "type": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                }
            }
        },
        "openstack_endpoint": {
            "_timestamp": {
                "enabled": true
            },
            "_source": {
                "enabled": true
            },
            "_id": {
                "path": "id"
            },
            "_parent": {
                "type": "openstack_service"
            },
            "properties": {
                "id": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "service_id": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "region": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "adminurl": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "publicurl": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                },
                "internalurl": {
                    "type": "string",
                    "analyzer": "custom_whitespace"
                }
            }
        }
    }
}
