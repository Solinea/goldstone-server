# password for 'admin' user in goldstone client
DJANGO_ADMIN_PASSWORD=

# email address for admininstrative emails
DJANGO_ADMIN_EMAIL=

# password for 'gsadmin' user in goldstone client
GOLDSTONE_TENANT_ADMIN_PASSWORD=

# password for the 'postgres' user in postgres
POSTGRES_PASSWORD=

# password for the 'goldstone' user in postgres
GOLDSTONE_PASSWORD=

# The container's e-mail host IP address.
EMAIL_HOST=

# The hostname to be used for the "password reset link" in the body of
# password-reset e-mails.
EXTERNAL_HOSTNAME=

#
# OpenStack Cloud settings
#

OS_REGION_NAME=RegionOne
OS_TENANT_NAME=admin
OS_USERNAME=admin
OS_PASSWORD=
OS_AUTH_URL=
OS_TENANT_ID=
#
# Goldstone Canary settings.  Provide image, flavor, and network IDs for
# your cloud.
#
GSC_FLIGHT_INTERVAL=5m
GSC_IMAGE_ID=
GSC_FLAVOR_ID=
GSC_NETWORK_ID=
GSC_LOG_LEVEL=INFO
GSC_LOG_FORMAT=GELF
GSC_GREYLOG_IP=gslog:5517
GSC_VOLUME_CHECKS=true
GSC_VOLUME_SIZE=2

#
# Do not modify values below here.
#

DJANGO_SETTINGS_MODULE=goldstone.settings.docker
DJANGO_ADMIN_USER=admin

# Task system settings
GOLDSTONE_REDIS_HOST=gstaskq
CELERY_LOGLEVEL=info
